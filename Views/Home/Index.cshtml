@model List<StockDataViewModel>

<h2>Stock Data for AAPL</h2>

@if (Model != null && Model.Any())
{
    <table class="table">
        <thead>
            <tr>
                <th>Date</th>
                <th>Open</th>
                <th>High</th>
                <th>Low</th>
                <th>Close</th>
                <th>Volume</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Date</td>
                    <td>@item.Open</td>
                    <td>@item.High</td>
                    <td>@item.Low</td>
                    <td>@item.Close</td>
                    <td>@item.Volume</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No stock data available.</p>
}
