@page "{symbol?}"
@model WebApplication2.Areas.Identity.Pages.StockDataModel


<h1>Stock Data for @RouteData.Values["symbol"]</h1>

@if (Model.StockData != null)
{
    <table border="1">
        <thead>
            <tr>
                <th>Date</th>
                <th>Open</th>
                <th>High</th>
                <th>Low</th>
                <th>Close</th>
                <th>Volume</th>
            </tr>
        </thead>
        <tbody>
            <!-- Parse the StockData string and display data in rows -->
            @* This is a simplified example; you'll need to parse the StockData string and populate this table accordingly *@
            <tr>
                <td>Date Value</td>
                <td>Open Value</td>
                <td>High Value</td>
                <td>Low Value</td>
                <td>Close Value</td>
                <td>Volume Value</td>
            </tr>
        </tbody>
    </table>
}
else
{
    <p>No stock data available for the provided symbol.</p>
}
